{
  "name": "node-api",
  "version": "1.0.0",
  "description": "Node API",
  "scripts": {
    "start": "npm run start-env-dev",
    "start-env-dev": "ts-node src/node-api --NODE_ENV=dev",
    "start-env-prod": "ts-node src/node-api --NODE_ENV=prd",
    "start:dev": "ts-node-dev --respawn src/node-api --NODE_ENV=dev",
    "start:debug": "nodemon --config nodemon-debug.json --NODE_ENV=dev",
    "test": "jest --colors --NODE_ENV=dev",
    "lint": "tslint --fix 'src/**/*.ts'",
    "db:migrate-all": "sequelize db:migrate",
    "db:seed-all": "sequelize db:seed:all"
  },
  "author": "Manoj Bandara",
  "license": "MIT",
  "dependencies": {
    "aspect.js": "^0.7.3",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "circular-json": "^0.5.9",
    "cluster": "^0.7.7",
    "collections": "^5.1.5",
    "compression": "^1.7.3",
    "config": "^2.0.1",
    "correlation-id": "^3.0.2",
    "cors": "^2.8.5",
    "eureka-js-client": "^4.4.1",
    "execution-time": "^1.3.0",
    "express": "^4.16.3",
    "express-unless": "^0.5.0",
    "glob": "^7.1.3",
    "inversify": "^4.13.0",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "^6.1.0",
    "js-yaml": "^3.12.0",
    "jsonpath": "^1.0.0",
    "jsonwebtoken": "^8.3.0",
    "load-balancers": "^1.3.2",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "mongodb": "^3.1.10",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "node-cache": "^4.2.0",
    "public-ip": "^3.0.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.16.0",
    "sequelize-cli": "^5.5.0",
    "sequelize-typescript": "^1.0.0",
    "swagger-express-ts": "^1.0.0",
    "swagger-ui-dist": "^3.19.2",
    "ts-custom-error": "^2.2.2",
    "typescript-collections": "^1.3.2",
    "xml-js": "^1.6.8"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx)"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/bunyan": "^1.8.4",
    "@types/compression": "0.0.36",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.4",
    "@types/eureka-js-client": "^4.3.1",
    "@types/express": "^4.16.0",
    "@types/express-unless": "0.0.32",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.9",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.119",
    "@types/mongodb": "^3.1.19",
    "@types/morgan": "^1.7.35",
    "@types/node-cache": "^4.1.3",
    "@types/public-ip": "^2.4.0",
    "@types/randomstring": "^1.1.6",
    "@types/validator": "^10.11.3",
    "axios-mock-adapter": "^1.16.0",
    "jest": "^24.1.0",
    "jest-create-mock-instance": "^1.1.0",
    "jest-mock-axios": "^2.1.11",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.30",
    "tsconfig-paths": "^3.6.0",
    "tslint": "^5.12.1",
    "typescript": "^3.1.1"
  }
}
